okenConfigs[underlying];\r\n\r\n        if(tokenConfig.fixedUsd){\r\n            return uint(1e36).div(tokenConfig.baseUnit);\r\n        }\r\n\r\n        uint256 price_0 = getSourcePrice(underlying, 0);\r\n        uint256 price_1 = getSourcePrice(underlying, 1);\r\n        require(price_0 > 0 && price_1 > 0);\r\n\r\n        bool flag = withinLimit(underlying, price_0, price_1);\r\n        require(flag, \"Out of limit\");\r\n\r\n        uint price = avg(price_0,price_1);\r\n\r\n         // price must bigger than 0\r\n        require(price > 0, \"price must bigger than zero\");\r\n        return price;\r\n    }\r\n\r\n    function getSourceUnderlyingPrice(MarketTokenInterface marketToken, uint index) public view returns (uint){\r\n        address underlying = marketToken.underlying();\r\n        return getSourcePrice(underlying, index);\r\n    }\r\n\r\n\r\n    function getSourcePrice(address underlying, uint index) internal view returns (uint){        \r\n        address source = tokenConfigs[underlying].sources[index];\r\n\r\n        bytes memory payload = abi.encodeWithSignature(\"getPrice(address)\", underlying);\r\n        bytes memory returndata = source.functionStaticCall(payload);\r\n        \r\n        uint _price =  abi.decode(returndata, (uint));\r\n        uint price = _price.mul(1e28).div(tokenConfigs[underlying].baseUnit);\r\n\r\n        return price;\r\n    }\r\n\r\n\r\n    struct MarketSourceView {\r\n        address source;\r\n        uint price;\r\n        string name;\r\n    }\r\n\r\n    function getMarketSources(MarketTokenInterface marketToken) public view returns(MarketSourceView[] memory) {\r\n        address underlying =  marketToken.underlying();\r\n        TokenConfig memory tokenConfig = tokenConfigs[underlying];\r\n\r\n        address[] memory sources = new address[](2);\r\n        MarketSourceView[] memory res = new MarketSourceView[](sources.length);\r\n\r\n        for(uint i = 0; i < sources.length; i++) {\r\n            address source = tokenConfig.sources[i];\r\n            uint price = source==0x0000000000000000000000000000000000000000 ? getPrice(underlying) : getSourcePrice(underlying, i);\r\n            string memory name = source==0x0000000000000000000000000000000000000000 ? \"WePiggy\" : abi.decode(source.functionStaticCall(abi.encodeWithSignature(\"name()\")), (string));\r\n            \r\n            MarketSourceView memory vars;\r\n            vars.name = name;\r\n            vars.price = price;\r\n            vars.source = source;\r\n\r\n            res[i] = vars;\r\n        }\r\n\r\n        return res;\r\n    }\r\n\r\n    function _setTokenConfigs(TokenConfig[] memory configs) public onlyOwner{\r\n        for(uint i = 0; i < configs.length; i++){\r\n           TokenConfig memory tokenConfig = configs[i];\r\n           _setConfig(tokenConfig.underlying, tokenConfig.sources, tokenConfig.baseUnit, tokenConfig.fixedUsd, tokenConfig.lowerLimit, tokenConfig.upperLimit);\r\n       }\r\n    }\r\n\r\n    function _setConfig(address underlying, address[2] memory sources, uint256 baseUnit, bool fixedUsd, uint lowerLimit, uint upperLimit) public onlyOwner{\r\n        TokenConfig storage tokenConfig = tokenConfigs[underlying];\r\n        tokenConfig.underlying = underlying;\r\n        tokenConfig.sources = sources;\r\n        tokenConfig.baseUnit = baseUnit;\r\n        tokenConfig.fixedUsd = fixedUsd;\r\n        tokenConfig.lowerLimit = lowerLimit;\r\n        tokenConfig.upperLimit = upperLimit;\r\n    }\r\n\r\n\r\n    function withinLimit(address underlying, uint price0, uint price1) internal view returns(bool){\r\n\r\n        TokenConfig memory tokenConfig = tokenConfigs[underlying];\r\n\r\n        uint lowerLimit = tokenConfig.lowerLimit;\r\n        uint upperLimit = tokenConfig.upperLimit;\r\n        uint limit = uint(1000000).mul(price0).div(price1);\r\n\r\n        if(limit < lowerLimit || limit > upperLimit){\r\n            return false;\r\n        }\r\n\r\n        return true;\r\n    }\r\n\r\n    function avg(uint price0, uint price1) internal pure returns(uint){\r\n        return price0.add(price1).div(2);\r\n    }\r\n\r\n\r\n}","ABI":"[{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"previousOwner\",\"type\":\"address\"},{\"indexed\":true,\"internalType\":\"address\",\"name\":\"newOwner\",\"type\":\"address\"}],\"name\":\"OwnershipTransferred\",\"type\":\"event\"},{\"inputs\":[{\"internalType\":\"address\",\"name\":\"underlying\",\"type\":\"address\"},{\"internalType\":\"address[2]\",\"name\":\"sources\",\"type\":\"address[2]\"},{\"internalType\":\"uint256\",\"name\":\"baseUnit\",\"type\":\"uint256\"},{\"internalType\":\"bool\",\"name\":\"fixedUsd\",\"type\":\"bool\"},{\"internalType\":\"uint256\",\"name\":\"lowerLimit\",\"type\":\"uint256\"},{\"internalType\":\"uint256\",\"name\":\"upperLimit\",\"type\":\"uint256\"}],\"name\":\"_setConfig\",\"outputs\":[],\"stateMutability\":\"nonpayable\",\"type\":\"function\"},{\"inputs\":[{\"components\":[{\"internalType\":\"address\",\"name\":\"underlying\",\"type\":\"address\"},{\"internalType\":\"address[2]\",\"name\":\"sources\",\"type\":\"address[2]\"},{\"internalType\":\"uint256\",\"name\":\"baseUnit\",\"type\":\"uint256\"},{\"internalType\":\"bool\",\"name\":\"fixedUsd\",\"type\":\"bool\"},{\"internalType\":\"uint256\",\"name\":\"lowerLimit\",\"type\":\"uint256\"},{\"internalType\":\"uint256\",\"name\":\"upperLimit\",\"type\":\"uint256\"}],\"internalType\":\"struct WePiggyCAAPriceOracle.TokenConfig[]\",\"name\":\"configs\",\"type\":\"tuple[]\"}],\"name\":\"_setTokenConfigs\",\"outputs\":[],\"stateMutability\":\"nonpayable\",\"type\":\"function\"},{\"inputs\":[{\"internalType\":\"contract MarketTokenInterface\",\"name\":\"marketToken\",\"type\":\"address\"}],\"name\":\"getMarketSources\",\"outputs\":[{\"components\":[{\"internalType\":\"address\",\"name\":\"source\",\"type\":\"address\"},{\"internalType\":\"uint256\",\"name\":\"price\",\"type\":\"uint256\"},{\"interna