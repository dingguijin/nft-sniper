56) public nonces;\r\n    // keccak256(bytes(\"EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)\"))\r\n    bytes32 public constant DOMAIN_TYPEHASH = 0x8b73c3c69bb8fe3d512ecc4cf759cc79239f7b179b0ffacaa9a75d522b39400f;\r\n    // keccak256(bytes(\"Permit(address owner,address spender,address token,uint256 value,uint256 nonce,uint256 deadline)\"))\r\n    bytes32 public constant PERMIT_TYPEHASH = 0xbdb8db57b2e31447bff640fcc287b05a31c3da45ca140eaa5ba8f3b09ac99506;\r\n\r\n    // === EVENT LOGGERS ===\r\n    event Profited(address indexed _buyer, address _vault, address _paymentToken, uint256 _amount);\r\n    event DefaultBpUpdated(uint256 _bp);\r\n    event AddressBpConfigured(address indexed _spender, uint256 _bp);\r\n    event AddressBpExemption(address indexed _spender, bool _exemption);\r\n\r\n    // === VIEWERS ===\r\n\r\n    /**\r\n     * @return multiplier - The basis point for a given spender\r\n     */\r\n    function getFeeMultiplier(address _spender) public view returns (uint256 multiplier) {\r\n        if (!_exemption[_spender]) {\r\n            if (_addressBp[_spender] == 0) {\r\n                multiplier = _bp;\r\n            } else {\r\n                multiplier = _addressBp[_spender];\r\n            }\r\n        }\r\n    }\r\n\r\n    // === STATE-CHANGING FUNCTIONS ===\r\n\r\n    /**\r\n     * @notice The caller of this function must obtain an EIP712 signature from the buyer to receive payment\r\n     * @param _buyer The buyer, this is most likely the user's address\r\n     * @param _token The address of the payment token\r\n     * @param _amount The amount of payment to be collected\r\n     * @param _deadline The timestamp which the transaction reverts\r\n     * @param _signature The EIP712 signature of the buyer\r\n     */\r\n    function collectPayment(\r\n        address _buyer,\r\n        address _token,\r\n        uint256 _amount,\r\n        uint256 _deadline,\r\n        bytes calldata _signature\r\n    ) external override {\r\n        require(_deadline >= block.timestamp, \"NFTFairTreasury: EXPIRED\");\r\n        Permit memory permit = Permit(_buyer, msg.sender, _token, _amount, nonces[_buyer]++, _deadline);\r\n        bytes32 digest = _getDigest(permit);\r\n        bytes32 message = _generateMessage(digest);\r\n        require(_sigIsValid(_buyer, message, _signature), \"NFTFairTreasury: INVALID_SIGNATURE\");\r\n        _collectPayment(_buyer, _token, _amount);\r\n    }\r\n\r\n    // === ADMIN ===\r\n\r\n    /**\r\n     * @notice Sets the default basis point for all spenders\r\n     */\r\n    function setNewBp(uint256 _newBp) external onlyOwner {\r\n        require(_newBp <= 3000, \"NFTFairTreasury: New BP can be set to 30% at most\");\r\n        _bp = _newBp;\r\n        emit DefaultBpUpdated(_newBp);\r\n    }\r\n\r\n    /**\r\n     * @notice Sets the basis point for a given spender\r\n     */\r\n    function setAddressBp(address _spender, uint256 _newBp) external onlyOwner {\r\n        require(_newBp <= 3000, \"NFTFairTreasury: New BP can be set to 30% at most\");\r\n        _addressBp[_spender] = _newBp;\r\n        emit AddressBpConfigured(_spender, _newBp);\r\n    }\r\n\r\n    /**\r\n     * @notice Exempts a given spender from the basis point fee\r\n     */\r\n    function toggleAddressExemption(address _spender) external onlyOwner {\r\n        _exemption[_spender] = !_exemption[_spender];\r\n        emit AddressBpExemption(_spender, _exemption[_spender]);\r\n    }\r\n\r\n    /**\r\n     * @notice Withdraws the protocol revenue from the treasury\r\n     * @param _recipient The beneficiary of the revenue\r\n     * @param _token The address of the payment token\r\n     * @param _amount The amount of revenue to be withdrawn\r\n     */\r\n    function recoverERC20(\r\n        address _recipient,\r\n        address _token,\r\n        uint256 _amount\r\n    ) external onlyOwner {\r\n        IERC20(_token).safeTransfer(_recipient, _amount);\r\n    }\r\n\r\n    // === INTERNAL FUNCTIONS ===\r\n\r\n    /**\r\n     * @notice This is an internal function that implements the mechanism for collecting payment\r\n     * The first step of this function is to determine the basis point fee for the caller\r\n     * If the caller is exempt, the entire amount is transferred to the caller.\r\n     * Otherwise, the basis point fee is calculated, stored in the treasury, with the remaining amount is transferred to the caller\r\n     */\r\n    function _collectPayment(\r\n        address _buyer,\r\n        address _token,\r\n        uint256 _amount\r\n    ) private {\r\n        uint256 profit = 0;\r\n        uint256 feeMultiplier = getFeeMultiplier(msg.sender);\r\n        if (feeMultiplier > 0) {\r\n            IERC20(_token).safeTransferFrom(_buyer, address(this), _amount);\r\n            profit = (_amount * (feeMultiplier)) / 10000;\r\n            _amount -= profit;\r\n            IERC20(_token).safeTransfer(msg.sender, _amount);\r\n            emit Profited(_buyer, msg.sender, _token, _amount);\r\n        } else {\r\n            IERC20(_token).safeTransferFrom(_buyer, msg.sender, _amount);\r\n        }\r\n    }\r\n\r\n    /**\r\n     * @notice This function computes the domain separator. The chain ID included in the domain separator is read directly on-chain to avoid collision, which could lead to cross-network replay attacks.\r\n     */\r\n    function _getDomain() private view returns (bytes32 DOMAIN_SEPARATOR) {\r\n        uint256 chainId;\r\n\r\n        assembly {\r\n            chainId := chainid()\r\n        }\r\n\r\n        DOMAIN_SEPARATOR = keccak256(\r\n            abi.encode(\r\n                DOMAIN_TYPEHASH,\r\n                keccak256(bytes(\"NFTFairTreasury\")),\r\n                keccak256(bytes(\"1\")),\r\n                chainId,\r\n                address(this)\r\n            )\r\n        );\r\n    }\r\n\r\n    function _getDigest(Permit memory permitContent) private pure returns (bytes32 PERMIT_DIGEST) {\r\n        PERMIT_DIGEST = keccak256(\r\n            abi.encode(\r\n                PERMIT_TYPEHASH,\r\n                permitContent.owner,\r\n                permitContent.spender,\r\n                permitContent.token,\r\n                permitContent.value,\r\n                permitContent.nonce,\r\n                permitContent.deadline\r\n            )\r\n        );\r\n    }\r\n\r\n    function _generateMessage(bytes32 _digest) private view returns (bytes32 _message) {\r\n        _message = keccak256(abi.encodePacked(\"\\x19\\x01\", _getDomain(), _digest));\r\n    }\r\n\r\n    function _sigIsValid(\r\n        address _from,\r\n        bytes32 _message,\r\n        bytes memory _sig\r\n    ) private pure returns (bool) {\r\n        address recovered = _message.recover(_sig);\r\n        return recovered == _from && recovered != address(0);\r\n    }\r\n}","ABI":"[{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"_spender\",\"type\":\"address\"},{\"indexed\":false,\"internalType\":\"uint256\",\"name\":\"_bp\",\"type\":\"uint256\"}],\"name\":\"AddressBpConfigured\",\"type\":\"event\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"_spender\",\"type\":\"address\"},{\"indexed\":false,\"internalType\":\"bool\",\"name\":\"_exemption\",\"type\":\"bool\"}],\"name\":\"AddressBpExemption\",\"type\":\"event\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":false,\"internalType\":\"uint256\",\"name\":\"_bp\",\"type\":\"uint256\"}],\"name\":\"DefaultBpUpdated\",\"type\":\"event\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"previousOwner\",\"type\":\"address\"},{\"indexed\":true,\"internalType\":\"address\",\"name\":\"newOwner\",\"type\":\"address\"}],\"name\":\"OwnershipTransferred\",\"type\":\"event\"},{\"anonymous\":false,\"inputs\":[{\"indexed\":true,\"internalType\":\"address\",\"name\":\"_buyer\",\"type\":\"address\"},{\"indexed\":false,\"internalType\":\"address\",\"nam