<?xml version="1.0" encoding="utf-8"?>

<templates>
  <t t-extend="ListView.buttons" t-name="ContractListView.buttons">
    <t t-jquery="button.o_list_button_add" t-operation="after">
      <button t-if="widget.modelName=='nft_sniper.raw_contract'" type="object" name="refresh" t-att-class="'d-none d-md-block btn' + (!widget.isMobile ? ' btn-secondary' : '') + ' o_button_refresh_contract'">
        Refresh
      </button>
      <button t-if="widget.modelName=='nft_sniper.raw_verified_contract'" type="object" name="refresh" t-att-class="'d-none d-md-block btn' + (!widget.isMobile ? ' btn-secondary' : '') + ' o_button_refresh_contract'">
        Refresh
      </button>
            
    </t>
  </t>
</templates>
